<%- include('../partials/header') %>

<section id="show-page">
  <div>Name: </div>
  <div><%= playlist.name %></div>
  <div><%= playlist.song %></div>
  <div>Rating: </div>
  <div><%= playlist.rating %></div>
  <a href="/playlists/<%= playlist._id %>/songs/new" <%- title === 'Add Song' ? 'class="active"' : '' %>> 
    ADD A SONG</a> 
  <% playlist.songs.forEach(function(s) { %>
    <%- s.link %>
<% }) %>
<%- include('../partials/footer') %>
